{
  "name": "ts-emeth",
  "version": "1.1.2",
  "description": "CSS Modules cli for emeth and TypeScript",
  "types": "src",
  "main": "./index",
  "license": "MIT",
  "browser": "./lib/browser.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jspears/ts-emeth.git"
  },
  "scripts": {
    "test-gen": "./bin/tse.js -p ./src/__test__/fixtures/**/*.cssm",
    "test": "$npm_execpath run test-gen && mocha -r ts-node/register ./src/__test__/**/*-test.ts*",
    "prepare": "tsc && $npm_execpath run test",
    "watch": "tsc -w"
  },
  "bin": {
    "tse": "./bin/tse.js"
  },
  "dependencies": {
    "@types/node": "^13.9.2",
    "@types/webpack": "^4.41.7",
    "@types/webpack-env": "^1.15.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "css-loader": "^3.4.2",
    "reflect-metadata": "^0.1.13",
    "ts-arg": "^1.0.3",
    "emeth": "^1.0.2",
    "@mrbuilder/plugin-emeth": "^4.0.0"
  },
  "devDependencies": {
    "nyc": "^15.0.0",
    "postcss": "^7.0.27",
    "webpack": "^4.42.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "mrbuilder": {
    "plugin": "./mrbuilder-plugin.js",
    "plugins": [
      "@mrbuilder/plugin-emeth"
    ]
  }
}
